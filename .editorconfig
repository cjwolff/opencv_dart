root = true

[*]
indent_style = space # "space"、"tab"
indent_size = 2
insert_final_newline = true
trim_trailing_whitespace = true

max_line_length = 100

[Makefile]
indent_style = tab # "space"、"tab"

[*.py]
indent_style = space # "space"、"tab"
indent_size = 4

[*.version]
insert_final_newline = false
